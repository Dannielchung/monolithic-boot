{
    "name" : "files-repository",
    "jndiName": "",
    "workspaces" : {
        "default" : "default",
        "allowCreation" : true
    },
    "storage" : {
        "binaryStorage" : {
            "type" : "file",
            "directory" : "@mvn.jcr.files@",
            "minimumBinarySizeInBytes" : 40
        }
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        }
    }
}